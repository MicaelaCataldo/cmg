@prefix : <http://w3id.org/medred/medred#ns>.
@prefix medred: <http://w3id.org/medred/medred#ns>.
@prefix schema: <https://schema.org/>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .


# Definizione delle classi
medred:Questionnaire rdf:type rdfs:Class .
medred:Question rdf:type rdfs:Class .
medred:Choice rdf:type rdfs:Class .
medred:Section rdf:type rdfs:Class .


# Definizione delle propriet√†

medred:isItemofSection rdf:type rdf:Property ;
                rdfs:subPropertyOf rdf:type ;
                rdfs:domain :Question ;
                rdfs:range :Questionnaire .


medred:items rdf:type rdf:Property ;
               rdfs:subPropertyOf rdf:type ;
               rdfs:domain :Section ;
               rdfs:range :Question .

medred:hasValue rdf:type rdf:Property ;
            rdfs:subPropertyOf rdf:type ;
            rdfs:domain :Question ;
            rdfs:range [ 
                rdf:type rdf:Datatype ;
                rdfs:subClassOf xsd:integer ;
                xsd:minInclusive "0"^^xsd:integer ;
                xsd:maxInclusive "3"^^xsd:integer 
            ] .

medred:dataValue rdf:type rdf:Property ;
                    rdfs:subPropertyOf rdf:type ;
                    rdfs:domain :Questionnaire ;
                    rdfs:range xsd:date .
